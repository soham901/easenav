{
  "name": "@easenav/core",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bunx tsup src/index.ts --watch --dts --format esm,cjs --clean",
    "build": "bunx tsup src/index.ts --dts --format esm,cjs --clean",
    "test": "bunx vitest run --passWithNoTests",
    "typecheck": "bunx tsc -p tsconfig.json --noEmit"
  },
  "peerDependencies": {}
}
